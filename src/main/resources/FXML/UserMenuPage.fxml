<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane onDragDone="#deSelectUser" onMouseClicked="#deSelectUser" prefHeight="344.0" prefWidth="592.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fms.Controllers.UserMenuController">
   <children>
      <AnchorPane layoutX="14.0" layoutY="14.0" prefHeight="217.0" prefWidth="250.0">
         <children>
            <ListView fx:id="userList" layoutY="7.0" onMouseClicked="#selectUser" prefHeight="260.0" prefWidth="336.0" />
         </children>
      </AnchorPane>
      <TextField fx:id="newUserName" layoutX="375.0" layoutY="48.0" promptText="Username" />
      <TextField fx:id="editUserName" layoutX="375.0" layoutY="169.0" promptText="Username" />
      <TextField fx:id="newUserAccess" layoutX="375.0" layoutY="112.0" promptText="Access Level Number" />
      <TextField fx:id="editUserAccess" layoutX="375.0" layoutY="233.0" promptText="Access Level Number" />
      <Label layoutX="375.0" layoutY="20.0" prefHeight="19.0" prefWidth="63.0" text="Add User:" />
      <Label fx:id="userInformationLabel" layoutX="14.0" layoutY="308.0" prefHeight="25.0" prefWidth="256.0" text="None" />
      <Label layoutX="375.0" layoutY="144.0" prefHeight="19.0" prefWidth="108.0" text="Edit Selected User:" />
      <Label layoutX="14.0" layoutY="282.0" prefHeight="19.0" prefWidth="108.0" text="Selected User:" />
      <Label layoutX="14.0" layoutY="5.0" prefHeight="19.0" prefWidth="63.0" text="User List:" />
      <PasswordField fx:id="newUserPassword" layoutX="375.0" layoutY="79.0" promptText="Password" />
      <PasswordField fx:id="editUserPassword" layoutX="375.0" layoutY="201.0" promptText="Password" />
      <Button fx:id="addButton" layoutX="549.0" layoutY="79.0" mnemonicParsing="false" onAction="#addUser" text="Add" />
      <Button fx:id="editButton" layoutX="549.0" layoutY="201.0" mnemonicParsing="false" onAction="#editUser" text="Edit" />
      <Button fx:id="deleteUserButton" layoutX="272.0" layoutY="308.0" mnemonicParsing="false" onAction="#removeUser" text="Delete User" />
      <Button fx:id="exitToMenuButton" layoutX="460.0" layoutY="2.0" mnemonicParsing="false" onAction="#exitToMainMenu" text="Back to Menu" />
      <MenuBar layoutX="560.0" layoutY="2.0">
         <menus>
            <Menu mnemonicParsing="false" text="X" fx:id="xButton">
               <items>
                  <MenuItem fx:id="saveButton" mnemonicParsing="false" onAction="#saveData" text="save" />
                  <MenuItem mnemonicParsing="false" onAction="#saveAndExit" text="save and exit" />
                  <MenuItem fx:id="exitButton" mnemonicParsing="false" onAction="#exitProgram" text="exit" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</AnchorPane>
